CREATE TABLE doctor (
    id_dokter SERIAL PRIMARY KEY,
    nama VARCHAR(255),
    spesialis VARCHAR(255),
    jadwal VARCHAR(255),
    hari VARCHAR(255),
    keahlian TEXT,
    organisasi VARCHAR(255),
    gambar VARCHAR(255)
);


CREATE TABLE services (
    id_service SERIAL PRIMARY KEY,
    service VARCHAR(255),
    deskripsi TEXT,
    manfaat TEXT,
    gambar VARCHAR(255),
    id_dokter SERIAL REFERENCES doctor(id_dokter)
);

